  {
    "id": "z_image_local",
    "name": "Z-Image (Local)",
    "driver": "z_image",
    "options": {
      "url": "http://192.168.0.100:6006/generate",
      "width": 1024,
      "height": 1024,
      "useProxy": false
    },
    "parameters": [
      {
        "name": "width",
        "label": "Width",
        "type": "number",
        "default": 1024,
