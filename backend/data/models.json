        "max": 2048,
        "step": 64
      },
      {
        "name": "height",
        "label": "Height",
        "type": "number",
        "default": 1024,
        "min": 256,
        "max": 2048,
        "step": 64
      },
      {
        "name": "num_inference_steps",
        "label": "Steps",
        "type": "number",
        "default": 9,
        "min": 1,
        "max": 100,
        "step": 1
      },
      {
        "name": "guidance_scale",
        "label": "Guidance Scale",
        "type": "number",
        "default": 0,
        "min": 0.0,
        "max": 20.0,
        "step": 0.5
      },
      {
        "name": "seed",
        "label": "Seed",
        "type": "number",
        "default": 42,
        "description": "-1 for random"
      }
    ]
